{
  "name": "aginun",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "lint-fix": "vue-cli-service lint",
    "vue": "vue-cli-service",
    "i18n:report": "vue-cli-service i18n:report --src 'src/**/*.?(js|ts|vue)' --locales 'src/i18n/messages/**/*.json'",
    "i18n:extract": "vue-i18n-extract-translations -v 'src/**/*.?(js|ts|vue)' -l 'src/i18n/messages/' --target 'nl' --fill '' --key 'i18n'",
    "prei18n:push": "npm run i18n:extract",
    "i18n:push": "node src/i18n/scripts/push-translations.js",
    "prei18n:pull": "node src/i18n/scripts/delete-translations.js",
    "i18n:pull": "node src/i18n/scripts/pull-translations.js",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watchAll",
    "test:e2e": "vue-cli-service test:e2e",
    "stylelint": "stylelint **/*.scss",
    "start": "vue-cli-service serve",
    "docker": "docker-compose --file docker/docker-compose.yml --file docker/docker-compose.local-override.yml --env-file ./docker/.env.local up"
  },
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "@voerro/vue-tagsinput": "^2.7.1",
    "@vueform/slider": "^2.0.7",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "core-js": "^3.19.2",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "v-calendar": "^2.3.4",
    "vee-validate": "^3.4.14",
    "vue": "^2.6.12",
    "vue-apollo": "^3.1.0",
    "vue-axios": "^3.4.0",
    "vue-cookies": "^1.7.4",
    "vue-i18n": "^8.26.7",
    "vue-i18n-extract-translations": "^0.1.3",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.5.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@fullhuman/vue-cli-plugin-purgecss": "^4.0.3",
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@types/bootstrap": "^5.1.6",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-e2e-cypress": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-plugin-vuex": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/composition-api": "^1.4.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "node-sass": "^5.0.0",
    "postcss": "^7.0.39",
    "sass": "^1.44.0",
    "sass-loader": "^10.1.1",
    "style-resources-loader": "^1.3.3",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "~4.4.4",
    "unfetch": "^4.2.0",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.11"
  }
}
